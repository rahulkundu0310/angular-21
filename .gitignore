# Excludes compiled build artifacts and transpiled output folders from Git
/dist
/tmp
/out-tsc
/bazel-out

# Bypasses Node runtime folder and ignores package manager error log files
/node_modules
npm-debug.log
yarn-error.log

# Ignores IDE workspace files and editor-specific config from git tracking
.idea/
.project
.classpath
.c9/
*.launch
.settings/
*.sublime-workspace

# Retains only VS Code specific configs and excludes full history contents
.vscode/*
!.vscode/settings.json
!.vscode/tasks.json
!.vscode/launch.json
!.vscode/extensions.json
.history/*

# Covers Angular Sass caches coverage logs lock files and all test outputs
/.angular/cache
.sass-cache/
/connect.lock
/coverage
/libpeerconnection.log
testem.log
/typings
__screenshots__/

# Strips OS-generated system metadata files that corrupt shared repository
.DS_Store
Thumbs.db
